{
    "name": "klevze/thumb",
    "description": "simple thumbnail generator for Laravel",
    "homepage": "https://github.com/klevze/thumb",
    "keywords": ["image", "gd", "resize", "laravel", "thumbnail", "gif", "jpg", "png", "webp"],
    "version": "0.1.3",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregor KlevÅ¾e",
            "email": "gregor@klevze.si"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing.",
        "ext-imagick": "to use Imagick based image processing."
    },
    "autoload": {
        "psr-4": {
            "Klevze\\Thumb\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Klevze\\Thumb\\ThumbServiceProvider"
            ],
            "aliases": {
                "Thumb": "Klevze\\Thumb\\Facades\\Thumb"
            }
        }
    },
    "minimum-stability": "stable"
}
