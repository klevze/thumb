{
    "name": "klevze/thumb",
    "description": "simple thumbnail generator for laravel",
    "homepage": "https://github.com/klevze/thumb",
    "keywords": ["image", "gd", "resize", "laravel", "thumbnail"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gregor Klev≈æe",
            "email": "gregor@klevze.si"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing.",
        "ext-imagick": "to use Imagick based image processing."
    },
    "autoload": {
        "psr-4": {
            "Klevze\\Thumb\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Klevze\\Thumb\\ImageServiceProvider"
            ],
            "aliases": {
                "Image": "Klevze\\Thumb\\Facades\\Thumb"
            }
        }
    },
    "minimum-stability": "stable"
}
